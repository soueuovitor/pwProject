<div class="modal fade" id="patrocinador-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">

        

            <!-- Begin # DIV Form -->
            <div id="div-forms">


                <!-- Begin | Register Form -->
                <form id="register-patron" method="post" enctype="multipart/form-data">

                    <div class="modal-body">

                        <div id="div-register-msg">
                            <center>
                                <span id="text-register-msg">Criar Patrocinador</span>
                            </center>
                        </div>
                        <input type="file" name="logo">
                        <input id="register_name_normal" class="form-control" type="text" name="name" placeholder="Nome completo" required>
                        <input id="register_valor" class="form-control" type="number" name="valor" placeholder="Valor Doado" min="0" required>
                    </div>
                    <div class="modal-footer">
                        <div>
                            <button type="submit" class="btn btn-primary btn-lg btn-block">Registar</button>
                        </div>
                    </div>
                </form>
                <!-- End | Register Form -->
               
            </div>
            <!-- End # DIV Form -->
		</div>
	</div>
</div>